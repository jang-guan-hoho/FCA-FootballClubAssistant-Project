<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fca.model.dao.ClubDao">
	<select id="selectClubAll" resultType="Club">
		SELECT *
		FROM club
	</select>
	
	<select id="selectClub" parameterType="int" resultType="Club">
		SELECT *
		FROM club
		WHERE club_id = #{clubId}
	</select>
	
	<select id="selectMembers" parameterType="int" resultType="Member">
		SELECT *
		FROM member
		WHERE club_id = #{clubId}
	</select>
	
	<insert id="insertMember" parameterType="Map">
		INSERT INTO member (club_id, user_id, position)
		VALUES (#{clubId}, #{userId}, #{position})
	</insert>
</mapper>